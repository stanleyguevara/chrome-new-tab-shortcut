{
    "manifest_version": 2,
    "name": "New Tab Shortcut",
    "permissions": [
          "tabs"
        ],
    "version": "1.0",
    "description": "Customize chrome shortcut to open a new tab",
    "commands": {
          "close-tab": {
            "suggested_key": {
              "default": "Alt+Z",
              "mac": "Ctrl+Z"
            },
            "description": "Open a new tab"
          }
        },
    "background": {
      "scripts": ["main.js"],
      "persistent": false
    }
}
